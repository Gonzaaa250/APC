@model IEnumerable<TesisPadel.Models.Usuario>

@{
    ViewData["Title"] = "Usuario";
}

<div>
    <h1 class="display-4 titulo bajar">Crear Jugador</h1>
    <div class="row">
        <div class="col-lg-12">
            <div class="col-md-6">
                <p>Nombre</p>
                <input type="text" class="from-control" id="Nombre" autocomplete="off">
                <p>Localidad</p>
                <input type="text" class="from-control" id="Localidad" autocomplete="off">
                <p>Edad</p>
                <input type="number" class="from-control" id="Edad" autocomplete="off">
            </div>
            <div class="col-md-6">
                <p>Telefono</p>
                <input type="number" class="from-control" id="Telefono" autocomplete="off">
                <p>DNI</p>
                <input type="number" class="from-control" id="DNI" autocomplete="off">
                <p>Categoria</p>
                <input type="text" class="from-control" id="Categoria" autocomplete="off">
            </div>
        </div>
    </div>
</div>
<button type="button" class="button-87" role="button">Crear</button>
</div>